<% if(success.length || error.length) {%>
    <div class="alert alert-success alert-dismissible fade show clo-6 offset-3 " role="alert" >
        <h3><%= success %> <%= error %></h3>
        <button
            type="button" 
            class=" btn-close"
            data-bs-dismiss="alert" 
            aria-label="btn-close"
        ></button>
    </div>
<% } %>